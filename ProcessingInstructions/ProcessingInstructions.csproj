<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Qdabra.ProcessingInstructions</AssemblyName>
    <Authors>Qdabra Software</Authors>
    <Version>0.1.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Product>Qdabra Processing Instruction Utilities</Product>
    <PackageId>Qdabra.ProcessingInstructions</PackageId>
